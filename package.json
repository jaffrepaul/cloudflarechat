{
  "name": "cloudflare-agent-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others --success first \"npm run start:worker\" \"npm run start:build-server\"",
    "start:worker": "vite dev",
    "start:build-server": "tsx watch build-server/index.ts",
    "cleanup": "node -e \"require('child_process').execSync('pkill -f \\\"tsx watch build-server\\\" || true; for p in {3001..3010}; do lsof -ti:$p 2>/dev/null | xargs kill -9 2>/dev/null || true; done', {stdio: 'inherit'})\"",
    "deploy": "vite build && wrangler deploy",
    "test": "vitest",
    "types": "wrangler types env.d.ts --include-runtime false",
    "format": "prettier --write .",
    "check": "prettier . --check && biome lint && tsc"
  },
  "keywords": [
    "cloudflare",
    "ai",
    "agents"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@cloudflare/vite-plugin": "1.13.7",
    "@cloudflare/vitest-pool-workers": "^0.9.7",
    "@cloudflare/workers-types": "^4.20250927.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^8.2.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "3.2.4",
    "wrangler": "^4.40.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.23",
    "@ai-sdk/google": "^2.0.17",
    "@ai-sdk/openai": "^2.0.38",
    "@ai-sdk/react": "^2.0.56",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "agents": "^0.2.6",
    "ai": "^5.0.56",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "marked": "^16.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "workers-ai-provider": "^2.0.0",
    "ws": "^8.18.0",
    "zod": "^3.25.76"
  }
}
